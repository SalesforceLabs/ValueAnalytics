          "fields" : [
            {
              "type" : "TEXT",
              "name" : "SourceRecordId",
              "label" : "SourceRecordId",
              "formulaExpression" : "case \r\nwhen contains(PAGE_URL,'?ws=%') then substr(PAGE_URL,instr(PAGE_URL,'%2F',1,4)+3,18) else null end",
              "precision" : 255,
              "defaultValue" : ""
            }
          ]
        }
      },
      "FORMULA4" : {
